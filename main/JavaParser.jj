BlockNode Block() :
{
    BlockNode b = new BlockNode(fileName, token.beginLine);
    b.statements = new ArrayList<BlockStatementNode>();
    BlockStatementNode s;
}
{
    <LBRACE> ( s=BlockStatement()
    { b.statements.add(s); } )* <RBRACE>
    {
        return b;
    }
